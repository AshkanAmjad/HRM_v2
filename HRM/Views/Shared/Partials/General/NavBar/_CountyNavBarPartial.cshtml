            e.preventDefault();
            $.get(_countyReportsUrl).done(function (result) {
                $("#main").html(result);
            });
        });
        //------------------------------------------------------------------------
        $("#countyManagementDocuments").off("click");
        $("#countyManagementDocuments").on("click", function (e) {
            e.preventDefault();
            $.get(_countyManagementDocumentsUrl).done(function (result) {
                $("#main").html(result);
            });
        });
        //------------------------------------------------------------------------
        $("#countyManagementArchivedDocuments").off("click");
        $("#countyManagementArchivedDocuments").on("click", function (e) {
            $.get(_countyManagementArchivedDocumentsUrl).done(function (result) {
                $("#main").html(result);
            });
        });
        //------------------------------------------------------------------------
    })
</script>